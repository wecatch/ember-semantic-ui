{
  "name": "ember-semantic-ui",
  "description": "Ember ui components based on semantic-ui",
  "version": "1.0.9",
  "options": {
    "paths": [
      "addon"
    ],
    "enabledEnvironments": ["development", "production"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "themedir" : "node_modules/yuidoc-ember-cli-theme",
    "helpers" : ["node_modules/yuidoc-ember-cli-theme/helpers.js"]
  }
}
