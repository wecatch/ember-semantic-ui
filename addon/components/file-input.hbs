{{#if this.multiple }}
    <input type="file" name="file" multiple>
{{else}}
    <input type="file" name="file" >
{{/if}}

{{#if (has-block-params)}}
    {{yield this.uploader this.start this.abort this.fileObject}}
{{else}}
    {{#unless this.autoUpload}}
        <UiButton @loading={{this.isUploading}} @class={{this.theme}} @click={{this.start}}>upload</UiButton>
    {{/unless}}
{{/if}}